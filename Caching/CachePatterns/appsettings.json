{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CacheSettings": {
    "DefaultExpirationMinutes": 10,
    "RefreshAheadThresholdMinutes": 2,
    "WriteBehindFlushIntervalSeconds": 5,
    "FullCacheRefreshIntervalMinutes": 30,
    "NearCacheSyncIntervalMinutes": 2,
    "L1CacheExpirationMinutes": 2,
    "L2CacheExpirationMinutes": 10
  },
  "MemoryCacheOptions": {
    "SizeLimit": 1000,
    "CompactionPercentage": 0.25
  }
}